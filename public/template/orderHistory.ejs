
<div ng-show="orderHistory.length == 0"><h3 class="text-danger">It seems you have not ordered anything so far</h3x></div>
 <div  class="thumbnail" ng-repeat="order in orderHistory">

<img style="float: left;" alt="Image" src="{{order.customProductImageLocation[0]}}" width="70px;" height="50px;">

<div style="margin-left: 80px;">
							<h4 class="text-info">
								{{order.customProductName}}
							</h4>
							<br>
							<p>
							 <strong>Order Time:</strong> <small>{{order.customProductOrderDetails.OrderTime}}</small>
							
						 <strong>Order Status:</strong> <small class="text-success">{{order.customProductOrderDetails.orderDetailStatus}}</small>
						
						 <a ng-disabled="order.customProductOrderDetails.orderDetailStatus.localeCompare('success') !== 0"   class="btn btn-info btn-xs navbar-right" href="#" style="margin-right: 10px;">Reorder</a>
							</p>
							<br>
						</div>


</div>

