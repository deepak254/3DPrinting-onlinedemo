                    <a href="/productGallery" class="btn btn-primary btn-lg "><i class="fa fa-cloud-download"></i>Browse Product Gallery</a>
                  </span> <!-- end .button -->
                  
                </div> <!-- end .content -->
              </div> <!-- end .top > .container> .row> .col-md-7 -->

